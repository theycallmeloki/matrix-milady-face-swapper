{
    "pipeline": {
        "name": "face-swapper"
    },
    "input": {
        "cross": [
            {
                "pfs": {
                    "glob": "/*",
                    "repo": "nonmilady"
                }
            },
            {
                "pfs": {
                    "glob": "/*",
                    "repo": "milady"
                }
            }
        ]
    },
    "transform": {
        "image_pull_secrets": [
            "laneonekey"
        ],
        "cmd": ["python3", "/face_swapper.py", "nonmilady", "milady", "3", "3"],
        "image": "laneone/edith-images"
    }
}
